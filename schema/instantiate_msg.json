{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "base",
    "mint_price",
    "tokens"
  ],
  "properties": {
    "base": {
      "$ref": "#/definitions/InstantiateMsg"
    },
    "mint_price": {
      "$ref": "#/definitions/MintPrice"
    },
    "tokens": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CustomNFT"
      }
    }
  },
  "definitions": {
    "CustomNFT": {
      "type": "object",
      "required": [
        "color_name",
        "family_color",
        "hex_code",
        "uri"
      ],
      "properties": {
        "color_name": {
          "type": "string"
        },
        "family_color": {
          "type": "string"
        },
        "hex_code": {
          "type": "string"
        },
        "uri": {
          "type": "string"
        }
      }
    },
    "InstantiateMsg": {
      "type": "object",
      "required": [
        "minter",
        "name",
        "symbol"
      ],
      "properties": {
        "minter": {
          "description": "The minter is the only one who can create new NFTs. This is designed for a base NFT that is controlled by an external program or contract. You will likely replace this with custom logic in custom NFTs",
          "type": "string"
        },
        "name": {
          "description": "Name of the NFT contract",
          "type": "string"
        },
        "symbol": {
          "description": "Symbol of the NFT contract",
          "type": "string"
        }
      }
    },
    "MintPrice": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "denom": {
          "type": "string"
        }
      }
    }
  }
}
